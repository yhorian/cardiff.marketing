{
  "name": "webcm",
  "version": "0.10.3",
  "description": "Demonstrative implementation of a web-based manager for utilising Managed Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudflare/webcm"
  },
  "author": "Cloudflare Managed Components Team <managedcomponents@cloudflare.com> (https://blog.cloudflare.com/zaraz-open-source-managed-components-and-webcm/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cloudflare/webcm"
  },
  "keywords": [
    "webcm",
    "managed-components",
    "cloudflare"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "webcm": "./lib/src/cli.js"
  },
  "main": "lib/src/index.js",
  "scripts": {
    "dev": "set TS_NODE_FILES=true && nodemon --exec \"node --loader ts-node/esm index.ts\"",
    "build": "tsc",
    "lint": "eslint --ext .ts,.js, lib",
    "lint:fix": "eslint --ext .ts,.js, lib --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --globals",
    "test:dev": "vitest --globals",
    "buildcheck": "npm run typecheck && npm run test && npm run lint"
  },
  "dependencies": {
    "cookies": "^0.8.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^20.0.0",
    "locreq": "^2.1.0",
    "pacote": "^13.6.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webcm": "^0.10.4",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@managed-components/types": "^1.3.7",
    "@types/cookies": "^0.7.7",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.0.0",
    "@types/pacote": "^11.1.5",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "vitest": "^0.15.2"
  }
}
